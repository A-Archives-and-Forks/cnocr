{
    "version": "1.3.0",

    "vocab_fp": "examples/label_cn.txt",
    "img_folder": "data/images",

    "gpus": 0,
    "epochs": 2,
    "batch_size": 64,
    "num_workers": 0,
    "pin_memory": false,
    "optimizer": "adam",
    "learning_rate": 1e-5,
    "weight_decay": 0,
    "lr_scheduler": {
        "name": "multi_step",
        "milestones": [2, 6, 12],
        "gamma": 0.5
    },
    "precision": 32,
    "limit_train_batches": 3,
    "limit_val_batches": 2,
    "pl_checkpoint_monitor": "complete_match_epoch",
    "pl_checkpoint_mode": "max"
}